{
    
  "Version": "2012-10-17",
  "Statement":[
      {
        "Effect": "Allow",
        "Action": "s3:CreateBucket",
        "Resource": "*"
        
      },

      {
        "Effect": "Deny",
        "Action": "s3:DeletBucket",
        "Resource": "*"
        
      },

      {

        "Effect": "Deny",
            "Action": "s3:ObjectBucket",
            "Resource": "*"
            
      }

  ]
}